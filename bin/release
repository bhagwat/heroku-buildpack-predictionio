#!/usr/bin/env bash
# bin/release <build-dir>

BUILD_DIR=$1

cat <<EOF
---
config_vars:
  PATH: .jdk/bin:.sbt_home/bin:/usr/local/bin:/usr/bin:/bin
  JAVA_OPTS: -Xmx384m -Xss512k -XX:+UseCompressedOops
  APP_TYPE: Prediction.IO
addons:
  heroku-postgresql:dev
default_process_types:
  web: pio eventserver
EOF
